super + shift + Return:
	alacritty

super + shift + q:
	bspc quit

super + shift + r:
	sh ~/.config/polybar/run.sh

super + shift + c:
	bspc node -c

super + {1-9}:
	bspc desktop -f "^{1-9}"

super + shift + {1-9}:
	bspc node -d "^{1-9}"

super + shift + s:
	sh ~/.config/sxhkd/reload.sh

super + shift + b:
	bspc wm -r

super + s:
	scrot

super + l:
	rofi -show run
